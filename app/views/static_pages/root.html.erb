			<div class="col-md-8 col-md-offset-2">
				<div class="mtb">
					<% if flash.now[:errors] %>
						<% flash[:errors].each do |error| %>
							<div class="error"><%= error %></div>
							<style>
							input:enabled { background: red }
							</style>
						<% end %>
					<% end %>
					<form id="new-user" action="/signup" method="post"> 
						<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

						<input type="email" name="user[email]" class="subscribe-input" placeholder="Enter your e-mail address..." required>
						<button class='btn btn-conf btn-green' type="submit">Sign up!</button>
					</form>
				</div><!--/mt-->
			</div>
		</div><!--/row-->
	</div><!--/container-->
